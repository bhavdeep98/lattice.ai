{
  "name": "lattice-aws-cdk",
  "version": "0.1.0",
  "description": "Contract-First AWS Infrastructure with CDK - AI generates simple JSON intents, Lattice handles complex AWS implementation",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:performance": "jest --testPathPattern=performance",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy --context environment=dev",
    "deploy:staging": "cdk deploy --context environment=staging",
    "deploy:prod": "cdk deploy --context environment=prod",
    "destroy": "cdk destroy",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "lint": "eslint . --ext .ts --max-warnings 30",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:check": "eslint . --ext .ts --max-warnings 0 || echo 'Linting failed but continuing...'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pipeline:analyze-threats": "node scripts/pipeline-utils.js analyze-threats",
    "pipeline:estimate-costs": "node scripts/pipeline-utils.js estimate-costs",
    "pipeline:summary": "node scripts/pipeline-utils.js deployment-summary",
    "pipeline:check-doc-only": "node scripts/pipeline-utils.js check-doc-only",
    "security:scan": "npm audit && npm run pipeline:analyze-threats",
    "pre-commit": "npm run lint && npm run format:check && npm run test",
    "pre-deploy": "npm run build && npm run test && npm run synth && npm run pipeline:analyze-threats"
  },
  "keywords": [
    "aws",
    "cdk",
    "infrastructure",
    "contract-first",
    "ai",
    "typescript",
    "cicd",
    "security",
    "threat-modeling"
  ],
  "author": "",
  "license": "BSL-1.1",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.3.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.3.0"
  }
}